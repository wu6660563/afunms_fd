<?xml version="1.0" encoding="UTF-8"?>
<settings>
	<reload_data_interval>5</reload_data_interval>
	<background>
		<color>FFFFFF</color>
		<alpha>100</alpha>
		<border_alpha>100</border_alpha>
		<border_color>7599d7</border_color>  
	</background>
	<plot_area>
		<border_alpha>0</border_alpha>
		<margins>
			<left>50</left>
			<right>50</right>
			<bottom>50</bottom>
			<top>50</top>
		</margins>
	</plot_area>
	<grid>
		<x>
			<color>8080ff</color>
			<alpha>50</alpha>
			<dash_length>15</dash_length>
			<approx_count>20</approx_count>
		</x>
		<y_left>
			<color>8080ff</color>
			<alpha>50</alpha>
		</y_left>
	</grid>
	<axes>
		<x>
			<width>1</width>
			<alpha>50</alpha>
			<color>8080ff</color>
		</x>
		<y_left>
			<width>1</width>
			<alpha>50</alpha>
			<color>8080ff</color>
		</y_left>
	</axes>
	<values>
		<x>
			<enabled>1</enabled>
		</x>
		<y_left>
			<enabled>1</enabled>
		</y_left>
	</values>
	<indicator>
  <zoomable>false</zoomable> 
  </indicator>
	<graphs>
		<graph>
			  <title>CPU利用率</title> 	
		      <color>80ff80</color>
		      <bullet>round</bullet>
    	</graph>
    </graphs>
    <labels>
	<label>
      <x></x>                                                
      <y>20</y>                                               
      <rotate></rotate>                                      
      <width></width>                                         
      <align>center</align>                                  
      <text_color></text_color>                               
      <text_size>16</text_size>                                 
      <text>                                                 
        <![CDATA[<b>CPU实时利用率</b>]]>
      </text>        
    </label>  
    </labels>  
	<legend>
		<enabled>0</enabled>
	</legend>
</settings>