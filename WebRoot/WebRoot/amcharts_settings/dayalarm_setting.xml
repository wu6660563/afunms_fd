<settings>
 <data_type>csv</data_type>                                    
  <csv_separator>;</csv_separator>  
  <background>
    <border_color>FFFFFF</border_color>
    <border_alpha>100</border_alpha>
  </background>
  <plot_area>
    <color>7599d7</color>
    <alpha>30</alpha>
    <margins>
      <left>40</left>
      <right>20</right>
      <top>20</top>
      <bottom>40</bottom>
    </margins>
  </plot_area>
  <grid>
    <x>
      <approx_count>10</approx_count>
    </x>
  </grid>
  <axes>
    <x>
      <width>1</width>
      <color>FFFFFF</color>
      <alpha>25</alpha>
    </x>
    <y_left>
      <width>1</width>
      <alpha>10</alpha>
    </y_left>
  </axes>
  <values>
  	<x>
  	 <frequency>2</frequency>
      <skip_first>0</skip_first>
      <skip_last>1</skip_last>
  	</x>
    <y_left>
      <skip_last>1</skip_last>
      <integers_only>1</integers_only>
    </y_left>
  </values>
  <legend>
    <enabled>0</enabled>
  </legend>
  <graphs>
    <graph>
      <title>今日每小时告警数(条)</title>
      <color>99FF33</color>
      <color_hover>6699FF</color_hover>
      <balloon_text_color>000000</balloon_text_color>
      <balloon_text><![CDATA[<b>{value}条</b><br>{description}]]></balloon_text>
      <bullet>round_outlined</bullet>
    </graph>
  </graphs>
  <labels>
    <label lid="0">
      <text><![CDATA[<b>今日每小时告警数(条)</b>]]></text>
      <x>75</x>
      <text_size>15</text_size>
    </label>
  </labels>
</settings>